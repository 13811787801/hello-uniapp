<view class="tabs"><scroll-view class="scroll-h" id="tab-bar" scroll-x="{{true}}" show-scrollbar="{{false}}" scroll-into-view="{{scrollInto}}"><block s-for="{{tabBars}}" s-for-item="tab" s-for-index="index" s-key="id"><view class="uni-tab-item" id="{{tab.id}}" data-current="{{index}}" data-event-opts="{{[['tap',[['ontabtap',['$event']]]]]}}" bindtap="__e"><text class="{{['uni-tab-item-title',tabIndex==index?'uni-tab-item-title-active':'']}}">{{tab.name}}</text></view></block></scroll-view><view class="line-h"></view><swiper class="swiper-box" style="flex:1;" current="{{tabIndex}}" duration="{{300}}" data-event-opts="{{[['change',[['ontabchange',['$event']]]]]}}" bindchange="__e"><block s-for="{{newsList}}" s-for-item="tab" s-for-index="index1" s-key="index1"><swiper-item class="swiper-item"><scroll-view class="scroll-v list" enableBackToTop="true" scroll-y="{{true}}" data-event-opts="{{[['scrolltolower',[['loadMore',[index1]]]]]}}" bindscrolltolower="__e"><block s-for="{{tab.data}}" s-for-item="newsitem" s-for-index="index2" s-key="id"><view><media-item vue-id="{{'3b406ba4-1-'+index1+'-'+index2}}" options="{{newsitem}}" data-event-opts="{{[['^close',[['close',[index1,index2]]]],['^click',[['goDetail',['$0'],[[['newsList','',index1],['data','id',newsitem.id]]]]]]]}}" bind:close="__e" bind:click="__e"></media-item></view></block><block s-if="{{tab.isLoading||tab.data.length>4}}"><view class="loading-more"><text class="loading-more-text">{{tab.loadingText}}</text></view></block></scroll-view></swiper-item></block></swiper></view>